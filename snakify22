a = int(input())
b = int(input())
c = int(input())
if a == b and b == c :
    print("3")
else :
    if a == b and b != c :
        print("2")
    else :
        if a == c and a != b :
            print("2")
        else :
            if b == c and c != a :
               print("2")
            else :
                print("0")
